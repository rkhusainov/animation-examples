<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.khusainov.rinat.animationdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MotionLayoutActivity"></activity>
        <activity android:name=".shared_element_transition.SharedElementFragmentContainerActivity" />
        <activity android:name=".shared_element_transition.SharedElementTransitionSecondActivity" />
        <activity android:name=".shared_element_transition.SharedElementTransitionActivity" />
        <activity android:name=".ConstraintLayoutAnimationActivity" />
        <activity android:name=".TransitionApiActivity" />
        <activity android:name=".AnimatedVectorDrawableActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>